    901          /*********************************************************************
    902           * @fn      afCnvtFloat_SP
    903           *
    904           * @brief   Converts float to semi-precision structure format
    905           *
    906           * @param   f - float value to convert from
    907           *
    908           * NOTE: This function will convert to the closest possible
    909           *       representation in a 16 bit format.  Meaning that
    910           *       the number may not be exact.  For example, 10.7 will
    911           *       convert to 10.69531, because .7 is a repeating binary
    912           *       number.  The mantissa for afSemiPrecision_t is 10 bits
    913           *       and .69531 is the 10 bit representative of .7.
    914           *
    915           * @return  SemiPrecision conversion.
    916           */
    917          afSemiPrecision_t afCnvtFloat_SP( float f )
    918          {
    919            afSemiPrecision_t sp = {0,0,0};
    920            unsigned long mantissa;
    921            unsigned int oldexp;
    922            int tempexp;
    923            float calcMant;
    924            unsigned long *uf;
    925          
    926            if ( f < 0 )
    927            {
    928              sp.sign = 1;
    929              f = f * -1;
    930            }
    931            else
    932              sp.sign = 0;
    933          
    934            if ( f == 0 )
    935            {
    936              sp.exponent = (unsigned int)0;
    937              sp.mantissa = (unsigned int)0;
    938            }
    939            else
    940            {
    941              uf = (void*)&f;
    942          
    943              mantissa = *uf & 0x7fffff;
    944              oldexp = (unsigned int)((*uf >> 23) & 0xff);
    945              tempexp = oldexp - 127;
    946          
    947              calcMant = (float)((float)(mantissa) / (float)(0x800000));
    948              mantissa = (unsigned long)(calcMant * 1024);
    949          
    950              sp.exponent = (unsigned int)(tempexp + 15);
    951              sp.mantissa = (unsigned int)(mantissa);
    952            }
    953          
    954            return ( sp );
    955          }
