    957          /*********************************************************************
    958           * @fn      afCnvtSP_Float
    959           *
    960           * @brief   Converts semi-precision structure format to float
    961           *
    962           * @param   sp - afSemiPrecision format to convert from
    963           *
    964           * @return  float
    965           */
    966          float afCnvtSP_Float( afSemiPrecision_t sp )
    967          {
    968            float a, b, c;
    969          
    970            if ( sp.exponent == 0 && sp.mantissa == 0 )
    971            {
    972              a = 0;
    973              b = 0;
    974            }
    975            else
    976            {
    977              a = (float)((float)1 + (float)((float)(sp.mantissa)/1024));
    978          
    979          #if defined( __MWERKS__ )
    980              b = powf( 2.0, (float)((float)sp.exponent - 15.0) );
    981          #else
    982              b = (float)pow( 2.0, sp.exponent - 15 );
    983          #endif
    984            }
    985          
    986            if ( sp.sign )
    987              c = a * b * -1;
    988            else
    989              c = a * b;
    990          
    991            return ( c );
    992          }
    993          #endif
